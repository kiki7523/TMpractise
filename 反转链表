void displayLink(Node* head,stack *stack)//遍历
{
    Node* p;
    p = head;
    while (p->next != nullptr)
    {
        pushStack(stack, p->data);
        p = p->next;
    }
}

void displayLink1(stack* stack)
{
    display(stack);

}

int main()//利用栈的特性
{
	  Node* p = createLink(5);
    stack* q = new stack;
    initStack(q);
    displayLink(p,q);
    displayLink1(q);

	return 0;
}
