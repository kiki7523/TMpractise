void displayLink(Node* head,stack *stack)//链表元素入栈
{
    Node* p;
    p = head;
    while (p->next != nullptr)
    {
        pushStack(stack, p->data);
        p = p->next;
    }
}

int main()//利用单链表和栈的函数
{   
    Node* p = createLink(5);
    stack* q = new stack;
    initStack(q);
    displayLink(p,q);
    display(q);
    return 0;
}
